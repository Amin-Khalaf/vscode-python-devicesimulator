{
  "name": "embedded-python",
  "displayName": "embedded-python",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.34.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:adafruit.helloSimulator",
    "onCommand:adafruit.runEmulator"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "adafruit.helloSimulator",
        "title": "Open Simulator",
        "category": "Adafruit"
      },
      {
        "command": "adafruit.runSimulator",
        "title": "Run the Simulator",
        "category": "Adafruit"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "npm-run-all compile:*",
    "compile:extension": "tsc",
    "compile:views": "webpack --mode development",
    "watch": "npm-run-all -p watch:*",
    "watch:extension": "tsc --watch",
    "watch:views": "webpack --watch --mode development",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test",
    "package": "vsce package"
  },
  "devDependencies": {
    "typescript": "^3.3.1",
    "vscode": "^1.1.28",
    "tslint": "^5.12.1",
    "@types/node": "^10.12.21",
    "@types/mocha": "^2.2.42",
    "@types/react": "16.8.18",
    "@types/react-dom": "16.8.4",
    "css-loader": "^1.0.0",
    "less": "^3.7.0",
    "less-loader": "^4.1.0",
    "npm-run-all": "^4.1.3",
    "react-scripts": "3.0.1",
    "style-loader": "^0.21.0",
    "ts-import-plugin": "^1.5.4",
    "ts-loader": "^4.4.2",
    "vsce": "^1.47.0",
    "webpack": "^4.15.1",
    "webpack-cli": "^3.0.8"
  },
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "svg-inline-react": "^3.1.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
